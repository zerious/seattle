{
  "name": "seattle",
  "version": "0.0.4",
  "description": "CLI for the Seattle Pub.js Module Standard",
  "dependencies": {
    "chug": "^0.2.0",
    "shellify": "^0.0.4"
  },
  "devDependencies": {
    "zeriousify": "^0.1.10",
    "exam": "^0.0.8",
    "istanbul": "0.2.11",
    "coveralls": "2.10.0"
  },
  "keywords": [
    "seattle"
  ],
  "main": "seattle.js",
  "bin": {
    "seattle": "seattle.js"
  },
  "engines": {
    "node": ">=0.2.6"
  },
  "scripts": {
    "test": "./node_modules/exam/exam.js",
    "cover": "istanbul cover ./node_modules/exam/exam.js",
    "report": "open coverage/lcov-report/index.html",
    "coveralls": "istanbul cover ./node_modules/exam/exam.js --report lcovonly -- -R spec && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "repository": "https://github.com/lighterio/seattle.git",
